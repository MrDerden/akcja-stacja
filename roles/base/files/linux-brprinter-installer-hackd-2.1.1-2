	   MESSAGE "wget $WGET_OP $CACHEFLG -P ${wkdir} $fulpath"
	 fi
	 wget $WGET_OP $CACHEFLG -P ${wkdir} $fulpath > /dev/null 2> /dev/null 
         if [ $? = '0' ];then 
           break
