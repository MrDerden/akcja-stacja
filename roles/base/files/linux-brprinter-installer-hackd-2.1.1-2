dellist=''
cpscanlibmodules(){
  for file in $1
  do
    lib64mod=/usr/lib64/$file
    libmod=/usr/lib/$file

    if [ -f $lib64mod ];then
      if [ -d /usr/lib ];then
        if [ ! -f $libmod ];then
           cp $lib64mod  $libmod 2> /dev/null
           if [ -f $libmod ];then
             dellist2=$(echo $dellist $libmod)
             dellist=$dellist2
           fi
        fi
      fi
    fi
  done
}


rmscanlibmodules(){
  for file in $dellist
  do
    for check in $*
    do
      chkresult=$(echo $file | grep $check\$ )
      if [ "$chkresult" !=  '' ];then
        echo rm  -f $file 
      fi
    done
    
  done
}






