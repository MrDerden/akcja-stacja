    modelnhuc=$MODEL
  else

    inputmodel=$1
    modelnhuc=$(echo $inputmodel | tr "[a-z]" "[A-Z]" | tr -d '-')

    if [ "${modelnhuc}" = '' ];then
     echo ERROR :$inputmodel , $1   ,  ${modelnhuc}
     #DBG_MSG  "exit 1"
     exit 0
