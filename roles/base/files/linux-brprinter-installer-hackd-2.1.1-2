    fi
  fi
  #--------------------------------------   

  if [ ! -f ${wkdir}/${modelnhuc} ];then
    #    DCP315C   -> DCP310CN
    dlresult=APPROX4
    modelnhuc2=$(echo $inputmodel | tr "[a-z]" "[A-Z]" | tr -d '-' |\
		sed -e s/"[A-Z][A-Z][A-Z][A-Z]$"/""/g         \
		    -e s/"[A-Z][A-Z][A-Z]$"/""/g              \
		    -e s/"[A-Z][A-Z]$"/""/g                   \
