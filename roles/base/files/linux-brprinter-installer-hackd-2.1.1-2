	 fi
	 wget $WGET_OP $CACHEFLG -P ${wkdir} $fulpath > /dev/null 2> /dev/null 
         if [ $? = '0' ];then 
           break
         fi
       fi  
       modelnhuc='___not_found___'
     done
  fi

  #--------------------------------------   
  if [ ! -f ${wkdir}/${modelnhuc} ];then
    #  DCP315C  -> DCP315
    dlresult=APPROX2
    modelnhuc=$(echo $inputmodel | tr "[a-z]" "[A-Z]" | tr -d '-' |\
		sed -e s/"[A-Z][A-Z][A-Z][A-Z]$"/""/g         \
		    -e s/"[A-Z][A-Z][A-Z]$"/""/g              \
		    -e s/"[A-Z][A-Z]$"/""/g                   \
