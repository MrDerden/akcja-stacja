get_inf_file(){
  SUFFIX1="N C D DN DW DWN DWT CN CW CDN CD CDW CDWT DNLT JN J CLN CLWN WR"
  DBG_MSG get_inf_file

  if [ $NODOWNLOAD = 1 ];then
    cp  $INFPATH  ${wkdir}
    modelnhuc=$MODEL
